{
  "name": "Julia",
  "image": "ghcr.io/julia-vscode/julia-devcontainer",

  "customizations": {
    "vscode": {
      "extensions": ["julialang.language-julia", "cweijan.vscode-office"]
    }
  },

  "postCreateCommand": "/julia-devcontainer-scripts/postcreate.jl",

  "remoteUser": "vscode",
  "postStartCommand": "julia install.jl"
}
